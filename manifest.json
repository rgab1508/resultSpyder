{
	"manifest_version": 2,
	"name": "ResultSpider",
	"version": "0.0.1",

	"description": "Crawls the result and stores it.",

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["index.js"]
		}
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": ["tabs"]
}
